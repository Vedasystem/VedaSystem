@model IEnumerable<VedaSystem.Domain.Models.Transmissao>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Titulo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SubTitulo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TextoAntesDoVideo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CodigoDeIncorporacaoDeVideo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TextoDepoisDoVideo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rodape)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ativo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Terapeuta)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Titulo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SubTitulo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TextoAntesDoVideo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CodigoDeIncorporacaoDeVideo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TextoDepoisDoVideo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Rodape)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ativo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Terapeuta.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
