<!DOCTYPE html>
<html lang='en'>
<head>
    <meta charset='utf-8' />
    @*<link href="/assets/demo-to-codepen.css" rel="stylesheet">*@
    <style>
        html, body {
            margin: 0;
            padding: 0;
            font-family: Arial, Helvetica Neue, Helvetica, sans-serif;
            font-size: 14px;
        }

        #calendar {
            max-width: 1100px;
            margin: 40px auto;
        }
    </style>

    <link href="~/lib/fullcalendar-5.5.1/lib/main.css" rel="stylesheet">
    <script src="~/lib/fullcalendar-5.5.1/lib/main.js"></script>
    <script src="~/lib/fullcalendar-5.5.1/lib/locales-all.js"></script>

    @*<script src="/assets/demo-to-codepen.js"></script>*@
    <script>

        document.addEventListener('DOMContentLoaded', function () {
            var calendarEl = document.getElementById('calendar');
            var calendar = new FullCalendar.Calendar(calendarEl, {
                initialView: 'dayGridMonth'
            });
            calendar.render();
        });

    </script>
</head>
<body>
    <div id='calendar'></div>
</body>

</html>
