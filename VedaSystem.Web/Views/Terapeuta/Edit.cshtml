@model VedaSystem.Application.ViewModels.TerapeutaViewModel
@using VedaSystem.Application.Utils
<!doctype html>
<html lang="pt-br">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, height=device-height, initial-scale=1">
    <base href="../" />

    <title>Terapeutas - Veda Admin</title>

    <link rel="stylesheet" type="text/css" href="~/lib/ace-v3.1.1/node_modules/nouislider/distribute/nouislider.min.css">
    <link rel="stylesheet" type="text/css" href="~/lib/ace-v3.1.1/node_modules/ion-rangeslider/css/ion.rangeSlider.min.css">
    <link rel="stylesheet" type="text/css" href="~/lib/ace-v3.1.1/node_modules/tiny-date-picker/tiny-date-picker.css">
    <link rel="stylesheet" type="text/css" href="~/lib/ace-v3.1.1/node_modules/tiny-date-picker/date-range-picker.css">
    <link rel="stylesheet" type="text/css" href="~/lib/ace-v3.1.1/node_modules/eonasdan-bootstrap-datetimepicker/build/css/bootstrap-datetimepicker.css">
    <link rel="stylesheet" type="text/css" href="~/lib/ace-v3.1.1/node_modules/bootstrap-colorpicker/dist/css/bootstrap-colorpicker.css">
    <link rel="stylesheet" type="text/css" href="~/lib/ace-v3.1.1/dist/css/ace-font.css">
    <link rel="stylesheet" type="text/css" href="~/lib/ace-v3.1.1/dist/css/ace.css">
    <link rel="icon" type="image/png" href="~/lib/ace-v3.1.1/assets/favicon.png" />
</head>
<body>
    <di1v class="page-content container container-plus">
        <div class="card acard mt-2 mt-lg-3">
            <div class="card-header">
                <h3 class="card-title text-125 text-primary-d2">
                    <i class="far fa-edit text-dark-l3 mr-1"></i>
                    Cadastrar Terapeuta
                </h3>
            </div>
            <div class="card-body px-3 pb-1">
                <form asp-action="Edit" class="mt-lg-3" autocomplete="off" enctype="multipart/form-data">
                    <div class="form-group row">
                        <div class="col-sm-3 col-form-label text-sm-right pr-0">
                            <label asp-for="NomeCompleto" for="nome_completo" class="mb-0">
                            </label>
                        </div>
                        <div class="col-sm-9">
                            <input type="text" asp-for="NomeCompleto" class="form-control col-sm-8 col-md-6" id="nome_completo" />
                            <span asp-validation-for="NomeCompleto" class="text-danger" id="danger_nome_completo"></span>
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-3 col-form-label text-sm-right pr-0">
                            <label asp-for="NomeDeUsuario" for="nome_usuario" class="mb-0">
                            </label>
                        </div>
                        <div class="col-sm-9">
                            <input type="text" asp-for="NomeDeUsuario" class="form-control col-sm-8 col-md-6" id="nome_usuario" />
                            <span asp-validation-for="NomeDeUsuario" class="text-danger" id="danger_nome_usuario"></span>
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-3 col-form-label text-sm-right pr-0">
                            <label asp-for="Email" for="email" class="mb-0">
                            </label>
                        </div>
                        <div class="col-sm-9">
                            <input type="email" asp-for="Email" class="form-control col-sm-8 col-md-6" id="email" />
                            <span asp-validation-for="Email" class="text-danger" id="danger_email"></span>
                        </div>
                    </div>
                    <div class="form-group row">
                        @{
                            var SenhaDescriptografada = Cripto<VedaSystem.Application.ViewModels.TerapeutaViewModel>.Descriptografar(Model.Senha);
                        }
                        <div class="col-sm-3 col-form-label text-sm-right pr-0"
                            <label asp-for="Senha" for="id-form-field-password-toggle" class="mb-0">
                            </label>
                        </div>
                        <div class="col-sm-9">
                            <div class="d-inline-flex align-items-center">
                                <input type="password" asp-for="Senha" class="form-control form-control-lg pr-5" placeholder="Senha" value="@SenhaDescriptografada" text="SenhaDescriptografada" spellcheck="false" id="id-form-field-password-toggle" />
                                <a href="#" id="toggle-password" class="btn btn-sm border-0 btn-white btn-h-light-orange btn-a-light-orange text-125 ml-n5 no-underline radius-1 d-style">
                                    <i class="fa fa-eye-slash text-90 d-n-active w-3"></i>
                                    <i class="fa fa-eye text-90 d-active w-3"></i>
                                </a>
                                <span asp-validation-for="Senha" class="text-danger" id="danger_id-form-field-password-toggle"></span>
                            </div>
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-3 col-form-label text-sm-right pr-0">
                            <label asp-for="ConfirmeSenha" for="id-form-field-confirm-password-toggle" class="mb-0">
                            </label>
                        </div>
                        <div class="col-sm-9">
                            <div class="d-inline-flex align-items-center">
                                <input type="password" asp-for="ConfirmeSenha" class="form-control form-control-lg pr-5" placeholder="Senha" value="@SenhaDescriptografada" spellcheck="false" id="id-form-field-confirm-password-toggle" />
                                <a href="#" id="toggle-password" class="btn btn-sm border-0 btn-white btn-h-light-orange btn-a-light-orange text-125 ml-n5 no-underline radius-1 d-style">
                                    <i class="fa fa-eye-slash text-90 d-n-active w-3"></i>
                                    <i class="fa fa-eye text-90 d-active w-3"></i>
                                </a>
                                <span asp-validation-for="ConfirmeSenha" class="text-danger" id="danger_id-form-field-confirm-password-toggle"></span>
                            </div>
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-3 col-form-label text-sm-right pr-0">
                            <label asp-for="Telefone" for="telefone" class="mb-0">
                            </label>
                        </div>
                        <div class="col-sm-4">
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text"><i class="fa fa-phone"></i></span>
                                </div>
                                <input type="text" asp-for="Telefone" class="form-control form-control-lg" id="telefone" />
                            </div>
                            <span asp-validation-for="Telefone" class="text-danger" id="danger_telefone"></span>
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-3 col-form-label text-sm-right pr-0">
                            <label asp-for="Celular" for="celular" class="mb-0">
                            </label>
                        </div>
                        <div class="col-sm-5">
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text"><i class="fa fa-mobile"></i></span>
                                </div>
                                <input type="text" asp-for="Celular" class="form-control form-control-lg" id="celular" />
                                <label class="mt-1 mt-sm-0 ml-sm-3">
                                    <input type="checkbox" asp-for="WhatsApp" class="ace-switch input-lg ace-switch-yesno bgc-purple-d1 text-grey-m2" checked="" id="whatsApp">
                                </label>
                                <label asp-for="WhatsApp" for="celular" class="mb-0"></label>
                            </div>
                            <span asp-validation-for="Celular" class="text-danger" id="danger_celular"></span>
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-3 col-form-label text-sm-right pr-0">
                            <label asp-for="Site" for="site" class="mb-0">
                            </label>
                        </div>
                        <div class="col-sm-9">
                            <input type="text" asp-for="Site" class="form-control col-sm-8 col-md-6" id="site" />
                            <span asp-validation-for="Site" class="text-danger" id="danger_site"></span>
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-3 col-form-label text-sm-right pr-0">
                            <label asp-for="Endereco" for="endereco" class="mb-0">
                            </label>
                        </div>
                        <div class="col-sm-9">
                            <input type="text" asp-for="Endereco" class="form-control col-sm-8 col-md-6" id="endereco" />
                            <span asp-validation-for="Endereco" class="text-danger" id="danger_endereco"></span>
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-3 col-form-label text-sm-right pr-0">
                            <label asp-for="Cep" for="cep" class="mb-0">
                            </label>
                        </div>
                        <div class="col-sm-4">
                            <input type="text" asp-for="Cep" class="form-control col-sm-8 col-md-6" id="cep" />
                            <span asp-validation-for="Cep" class="text-danger" id="danger_cep"></span>
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-3 col-form-label text-sm-right pr-0">
                            <label asp-for="Estado" for="estado" class="mb-0">
                            </label>
                        </div>
                        <div class="col-sm-9">
                            <input type="text" asp-for="Estado" class="form-control col-sm-8 col-md-6" id="estado" />
                            <span asp-validation-for="Estado" class="text-danger" id="danger_estado"></span>
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-3 col-form-label text-sm-right pr-0">
                            <label asp-for="Cidade" for="cidade" class="mb-0">
                            </label>
                        </div>
                        <div class="col-sm-9">
                            <input type="text" asp-for="Cidade" class="form-control col-sm-8 col-md-6" id="cidade" />
                            <span asp-validation-for="Cidade" class="text-danger" id="danger_cidade"></span>
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-3 col-form-label text-sm-right pr-0">
                            <label asp-for="Bairro" for="bairro" class="mb-0">
                            </label>
                        </div>
                        <div class="col-sm-9">
                            <input type="text" asp-for="Bairro" class="form-control col-sm-8 col-md-6" id="bairro" />
                            <span asp-validation-for="Bairro" class="text-danger" id="danger_bairro"></span>
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-3 col-form-label text-sm-right pr-0">
                            <label asp-for="CPF" for="id-form-field-cpf-1" class="mb-0">
                            </label>
                        </div>
                        <div class="col-sm-4">
                            <input type="text" asp-for="CPF" class="form-control col-sm-8 col-md-6" id="id-form-field-cpf-1" />
                            <span asp-validation-for="CPF" class="text-danger" id="danger_id-form-field-cpf-1"></span>
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-3 col-form-label text-sm-right pr-0">
                            <label asp-for="CNPJ" for="id-form-field-cnpj-1" class="mb-0">
                            </label>
                        </div>
                        <div class="col-sm-4">
                            <input type="text" asp-for="CNPJ" class="form-control col-sm-8 col-md-6" id="id-form-field-cnpj-1" />
                            <span asp-validation-for="CNPJ" class="text-danger" id="danger_id-form-field-cnpj-1"></span>
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-3 col-form-label text-sm-right pr-0">
                            <label asp-for="DataNascimento" for="data_nascimento" class="mb-0">
                            </label>
                        </div>
                        <div class="col-sm-5">
                            @{
                                var dataNascimento = (DateTime)Model.DataNascimento;
                                var data = dataNascimento.ToString("yyyy-MM-dd");
                            }
                            <input type="date" asp-for="DataNascimento" class="form-control col-sm-8 col-md-6" id="data_nascimento" value='@data' />
                            <span asp-validation-for="DataNascimento" class="text-danger" id="danger_data_nascimento"></span>
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-3 col-form-label text-sm-right pr-0">
                            <label asp-for="ImagemUpload" for="imagem_upload" class="mb-0">
                            </label>
                        </div>
                        <div class="col-sm-6">
                            <input type="file" asp-for="ImagemUpload" class="form-control col-sm-8 col-md-6" id="imagem_upload" />
                            <span asp-validation-for="ImagemUpload" class="text-danger" id="danger_imagem_upload"></span>
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-3 col-form-label text-sm-right pr-0">
                            <label asp-for="Apresentacao" for="apresentacao" class="mb-0">
                            </label>
                        </div>
                        <div class="col-sm-9">
                            <input type="text" asp-for="Apresentacao" class="form-control col-sm-8 col-md-6" id="apresentacao" />
                            <span asp-validation-for="Apresentacao" class="text-danger" id="danger_apresentacao"></span>
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-3 col-form-label text-sm-right pr-0">
                            <label asp-for="Observacoes" for="observacoes" class="mb-0">
                            </label>
                        </div>
                        <div class="col-sm-9">
                            <input type="text" asp-for="Observacoes" class="form-control col-sm-8 col-md-6" id="observacoes" />
                            <span asp-validation-for="Observacoes" class="text-danger" id="danger_observacoes"></span>
                        </div>
                    </div>
                    <div class="mt-5 border-t-1 bgc-secondary-l4 brc-secondary-l2 py-35 mx-n25">
                        <div class="offset-md-3 col-md-9 text-nowrap">
                            <button class="btn btn-info btn-bold px-4" type="button" onclick="editTerapeuta()">
                                <i class="fa fa-check mr-1"></i>
                                Atualizar
                            </button>
                        </div>
                    </div>
                    @Html.HiddenFor(x => x.Id)
                </form>
            </div>
        </div>
    </di1v>
    <footer class="footer d-none d-sm-block">
        <div class="footer-inner bgc-white-tp1">
            <div class="pt-3 border-none border-t-3 brc-grey-l2 border-double">
                <span class="text-primary-m1 font-bolder text-120">Veda</span>
                <span class="text-grey">Application &copy; 2021</span>
                <span class="mx-3 action-buttons">
                    <a href="#" class="text-blue-m2 text-150"><i class="fab fa-twitter-square"></i></a>
                    <a href="#" class="text-blue-d2 text-150"><i class="fab fa-facebook"></i></a>
                    <a href="#" class="text-orange-d1 text-150"><i class="fa fa-rss-square"></i></a>
                </span>
            </div>
        </div>
        <div class="footer-tools">
            <a href="#" class="btn-scroll-up btn btn-dark mb-2 mr-2">
                <i class="fa fa-angle-double-up mx-2px text-95"></i>
            </a>
        </div>
    </footer>
    <footer class="d-sm-none footer footer-sm footer-fixed">
        <div class="footer-inner">
            <div class="btn-group d-flex h-100 mx-2 border-x-1 border-t-2 brc-primary-m3 bgc-white-tp1 radius-t-1 shadow">
                <button class="btn btn-outline-primary btn-h-lighter-primary btn-a-lighter-primary border-0" data-toggle="modal" data-target="#id-ace-settings-modal">
                    <i class="fas fa-sliders-h text-blue-m1 text-120"></i>
                </button>
                <button class="btn btn-outline-primary btn-h-lighter-primary btn-a-lighter-primary border-0">
                    <i class="fa fa-plus-circle text-green-m1 text-120"></i>
                </button>
                <button class="btn btn-outline-primary btn-h-lighter-primary btn-a-lighter-primary border-0" data-toggle="collapse" data-target="#navbarSearch" aria-controls="navbarSearch" aria-expanded="false" aria-label="Toggle navbar search">
                    <i class="fa fa-search text-orange text-120"></i>
                </button>
                <button class="btn btn-outline-primary btn-h-lighter-primary btn-a-lighter-primary border-0 mr-0">
                    <span class="pos-rel">
                        <i class="fa fa-bell text-purple-m1 text-120"></i>
                        <span class="badge badge-dot bgc-red position-tr mt-n1 mr-n2px"></span>
                    </span>
                </button>
            </div>
        </div>
    </footer>
    <script src="~/lib/ace-v3.1.1/node_modules/autosize/dist/autosize.js"></script>
    <script src="~/lib/ace-v3.1.1/node_modules/bootstrap-maxlength/dist/bootstrap-maxlength.js"></script>
    <script src="~/lib/ace-v3.1.1/node_modules/inputmask/dist/jquery.inputmask.js"></script>
    <script src="~/lib/ace-v3.1.1/node_modules/nouislider/distribute/nouislider.js"></script>
    <script src="~/lib/ace-v3.1.1/node_modules/ion-rangeslider/js/ion.rangeSlider.js"></script>
    <script src="~/lib/ace-v3.1.1/node_modules/bootstrap-touchspin/dist/jquery.bootstrap-touchspin.js"></script>
    <script src="~/lib/ace-v3.1.1/node_modules/tiny-date-picker/dist/date-range-picker.js"></script>
    <script src="~/lib/ace-v3.1.1/node_modules/moment/moment.js"></script>
    <script src="~/lib/ace-v3.1.1/node_modules/eonasdan-bootstrap-datetimepicker/src/js/bootstrap-datetimepicker.js"></script>
    <script src="~/lib/ace-v3.1.1/node_modules/bootstrap-colorpicker/dist/js/bootstrap-colorpicker.js"></script>
    <script src="~/lib/ace-v3.1.1/node_modules/es6-object-assign/dist/object-assign-auto.js"></script>
    <script src="~/lib/ace-v3.1.1/node_modules/jaames/iro/dist/iro.js"></script>
    <script src="~/lib/ace-v3.1.1/node_modules/jquery-knob/dist/jquery.knob.min.js"></script>
    <script src="~/lib/ace-v3.1.1/dist/js/ace.js"></script>
    <script src="~/lib/ace-v3.1.1/app/browser/demo.js"></script>
    <script src="~/lib/ace-v3.1.1/views/pages/form-basic/page-script.js"></script>
</body>
</html>












