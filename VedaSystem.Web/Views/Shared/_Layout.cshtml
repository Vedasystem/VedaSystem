<script>
    var ids = "";

//#region Terapeutas
function carregarObjetoTerapeuta() {
  var TerapeutaViewModel = {
    Id: '@Guid.NewGuid()',
    NomeCompleto: $("#nome_completo").val(),
    NomeDeUsuario: $("#nome_usuario").val(),
    Email: $("#email").val(),
    Senha: $("#id-form-field-password-toggle").val(),
    ConfirmeSenha: $("#id-form-field-confirm-password-toggle").val(),
    Telefone: $("#telefone").val(),
    Celular: $("#celular").val(),
    WhatsApp: $("#whatsApp").val(),
    Site: $("#site").val(),
    Endereco: $("#endereco").val(),
    Cep: $("#cep").val(),
    Estado: $("#estado").val(),
    Cidade: $("#cidade").val(),
    Bairro: $("#bairro").val(),
    CPF: $("#id-form-field-cpf-1").val(),
    CNPJ: $("#id-form-field-cnpj-1").val(),
    Apresentacao: $("#apresentacao").val(),
    Observacoes: $("#observacoes").val(),
    TipoPessoa: 2,
    DataCadastro: '@DateTime.Now.ToString("yyyy-MM-dd HH:mm:ss")',
    DataNascimento: $("#data_nascimento").val(),
    ImagemUpload: $("#imagem_upload").val(),
    Logo: null,
    Terapias: null,
    Precricoes: null,
    Pacientes: null
  };
  return TerapeutaViewModel;
}
function openTerapeutas() {
  $('body').addClass("loading");
  $.ajax({
    url: '@Url.Action("Index", "Terapeuta")',
    type: "GET",
    success: function (data) {
      var retorno = data;
      if (retorno.view.result === undefined) {
        $("#div-principal").html("");
      } else {
        $("#div-principal").html(retorno.view.result);
        $('.nav-item').removeClass("active");
        $('#li-terapeuta').addClass("active");
      }
      $('body').removeClass("loading");
    },
    error: function (result) {
    }
  });
}
function addTerapeutas() {
  $('body').addClass("loading");
  $.ajax({
    url: '@Url.Action("Create", "Terapeuta")',
    type: "GET",
    success: function (data) {
      var retorno = data;
      if (retorno.view.result === undefined) {
        $("#div-principal").html("");
      } else {
        $("#div-principal").html(retorno.view.result);

        $("#telefone").inputmask("(99)9999-9999")
        $("#celular").inputmask("(99)99999-9999")
        $("#cep").inputmask("99999-999")
        $("#id-form-field-cpf-1").inputmask("999.999.999-99")
        $("#id-form-field-cnpj-1").inputmask("99.999.999/9999-99")
        $("#data_nascimento").inputmask("99/99/9999")
      }
      $('body').removeClass("loading");
    },
    error: function (result) {
    }
  });
}
function createTerapeuta() {
  $('body').addClass("loading");

  if (validaDadosTerapeuta() == false) {

    var entity = JSON.stringify(carregarObjetoTerapeuta())

    $.ajax({
      url: '@Url.Action("CreateWithJson", "Terapeuta")',
      type: "GET",
      data: { entity: entity },
      dataType: "json",
      success: function (data) {
        var retorno = data;
        if (retorno.view.result === undefined) {
          $("#div-principal").html("");
        } else {
          $("#div-principal").html(retorno.view.result);
          $('.nav-item').removeClass("active");
          $('#li-terapeuta').addClass("active");
        }
        $('body').removeClass("loading");
      },
      error: function (result) {
      }
    });
  } else {
    $('body').removeClass("loading");
  }
}
function validaDadosTerapeuta() {
  $("#nome_completo").val() == "" ? $("#danger_nome_completo").removeClass("field-validation-valid").addClass("field-validation-error").text("O preenchimento do campo Nome Completo é obrigatório") : $("#danger_nome_completo").removeClass("field-validation-error").addClass("field-validation-valid").text("");
  $("#nome_usuario").val() == "" ? $("#danger_nome_usuario").removeClass("field-validation-valid").addClass("field-validation-error").text("O preenchimento do campo Nome de Usuario é obrigatório") : $("#danger_nome_usuario").removeClass("field-validation-error").addClass("field-validation-valid").text("");
  $("#email").val() == "" ? $("#danger_email").removeClass("field-validation-valid").addClass("field-validation-error").text("O preenchimento do campo Email é obrigatório") : $("#danger_email").removeClass("field-validation-error").addClass("field-validation-valid").text("");
  $("#id-form-field-password-toggle").val() == "" ? $("#danger_id-form-field-password-toggle").removeClass("field-validation-valid").addClass("field-validation-error").text("O preenchimento do campo Senha é obrigatório") : $("#danger_id-form-field-password-toggle").removeClass("field-validation-error").addClass("field-validation-valid").text("");
  $("#id-form-field-confirm-password-toggle").val() == "" ? $("#danger_id-form-field-confirm-password-toggle").removeClass("field-validation-valid").addClass("field-validation-error").text("O preenchimento do campo Confirmar Senha é obrigatório") : $("#danger_id-form-field-confirm-password-toggle").removeClass("field-validation-error").addClass("field-validation-valid").text("");
  $("#celular").val() == "" ? $("#danger_celular").removeClass("field-validation-valid").addClass("field-validation-error").text("O preenchimento do campo Celular é obrigatório") : $("#danger_celular").removeClass("field-validation-error").addClass("field-validation-valid").text("");
